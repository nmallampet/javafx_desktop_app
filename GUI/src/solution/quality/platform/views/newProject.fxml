<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="200.0" prefWidth="500.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="solution.quality.platform.views.NewProjectController">
    <stylesheets>
        <URL value="@mainCascade.css" />
    </stylesheets>
   <children>
      <Label layoutX="14.0" layoutY="29.0" text="Project Name:">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="66.0" text="Project Location:">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="projNameTF" layoutX="110.0" layoutY="25.0" onKeyPressed="#onEnterPressed" prefHeight="25.0" prefWidth="200.0" styleClass="tf-underLine" stylesheets="@mainCascade.css" />
      <TextField fx:id="projLocationTF" layoutX="14.0" layoutY="86.0" prefHeight="25.0" prefWidth="382.0">
         <font>
            <Font size="10.0" />
         </font>
      </TextField>
      <Button fx:id="browseB" layoutX="400.0" layoutY="86.0" mnemonicParsing="false" onAction="#chooseProjLocation" text="Browse" />
      <Button fx:id="createB" layoutX="220.0" layoutY="155.0" mnemonicParsing="false" onAction="#createProj" text="Create" />
   </children>
</AnchorPane>
