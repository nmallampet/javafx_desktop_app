<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="500.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.65" fx:controller="solution.quality.platform.views.AddSuiteController">
    <stylesheets>
        <URL value="@mainCascade.css" />
    </stylesheets>
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Suite Name:">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="suiteNameTF" layoutX="94.0" layoutY="12.0" onKeyPressed="#onEnterPressed" prefHeight="25.0" prefWidth="235.0" promptText="Enter Suite Name" styleClass="tf-underLine" />
      <Label layoutX="14.0" layoutY="58.0" text="Design Location:">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="suiteLocTF" layoutX="14.0" layoutY="78.0" prefHeight="25.0" prefWidth="380.0" promptText="Location of Design with testcases" />
      <Button fx:id="browseB" layoutX="400.0" layoutY="78.0" mnemonicParsing="false" onAction="#chooseProjLocation" text="Browse" />
      <TextArea fx:id="InfoTA" layoutX="14.0" layoutY="153.0" prefHeight="40.0" prefWidth="380.0" promptText="N/A" />
      <Label layoutX="14.0" layoutY="133.0" text="Detected TestCases:">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label fx:id="daisyChainL" layoutX="14.0" layoutY="212.0" text="daisychain" textFill="RED">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button fx:id="createB" layoutX="224.0" layoutY="261.0" mnemonicParsing="false" onAction="#createButtonClick" text="Create!" />
   </children>
</AnchorPane>
